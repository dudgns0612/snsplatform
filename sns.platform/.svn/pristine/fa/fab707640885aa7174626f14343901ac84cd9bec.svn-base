}	

#carousel-example-generic{
	position: absolute;
	width : 300px;
	height: 300px;
	top : 150px;
	left: 90px;
	align : center;
}

 .carousel-inner > .item > img {
      top: 0;
      left: 0;
      min-width: 100%;
      min-height : 300px;
      max-height: 300px;
} 
