.board-content-form{
	position : absolute;
	width : 100%;
	height : 500px;
	padding : 15px 30px 30px 30px;
	position: relative;
	max-height : 500px;
	overflow: auto;
}

.board-title{

}

.board-user-image{
}

.boardUserImage{
	position : absolute;
	padding-left : 50px;
	width : 170px !important;
	height: 120px;
	border-radius: 10px;
	left : 40px;
}

.board-creator{
	position : absolute;
	left : 220px;
	font-family: "Roboto", sans-serif;
	font-size : 17px;
	
}

.board-btn{
	font-family: "Roboto", sans-serif;
	text-transform: uppercase;
	width: 15%;
	border : 0px;
	margin-top : 10px;
	padding: 10px;
	color: #FFFFFF;
	background: #4A8C99;
	font-size: 15px;
	cursor: pointer; /*커서모양을 바꿈*/
	border-radius: 8px;
}

.modifybtn{
	position: absolute;
	left : 560px;
}	

.deletebtn{
